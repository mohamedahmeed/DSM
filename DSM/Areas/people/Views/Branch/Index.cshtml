@using DSM.TABLES

@model DSM.DTO.resultDTO

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a href="http://localhost:4540/people/branch/Add">Create New</a>
</p>
<table class="table">
